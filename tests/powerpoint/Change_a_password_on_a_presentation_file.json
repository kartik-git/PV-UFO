{
    "task": "Change a password on a presentation file",
    "steps": [
        "1. Open PowerPoint.",
        "2. Open a presentation.",
        "3. Click on the 'File' tab.",
        "4. Click on 'Info'.",
        "5. Click on 'Protect Presentation'.",
        "6. Click on 'Encrypt with Password'.",
        "7. Enter the current password.",
        "8. Click on 'OK'.",
        "9. Enter a new password.",
        "10. Click on 'OK'.",
        "11. Re-enter the new password.",
        "12. Click on 'OK'."
    ],
    "object": "Presentation1.pptx"
}